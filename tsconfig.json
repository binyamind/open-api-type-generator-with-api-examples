{
 "compilerOptions": {
   "module": "commonjs",
   "jsx": "react",
   "target": "ES6",
   "moduleResolution": "node",
   "resolveJsonModule": true,
   "outDir": "./build",
   "allowJs": true,
   "strict": true,
   "skipLibCheck": true,
   "esModuleInterop": true,
   "forceConsistentCasingInFileNames": true,
   "noImplicitThis": true,
   "noImplicitReturns": true,
   "noImplicitAny": true,
   "types": ["node"],
 },
 "include": ["src"]
}
